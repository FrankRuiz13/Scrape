<div class="container">
    {{#each articles}}
    <div data-id="{{this._id}} class="card">
        <div class="card-header">
            <h3><a class="article-link" target ="_black" href={{this.link}}>{{this.title}</a>}</h3>
            <a class="btn btn-success save">Save Article</a>
        </div>

    </div>
    {{/each}}
    {{#unless articles.length}}
        <div class="card-header">
            <h3>No aritcles yet</h3>
        </div>
    {{/unless}}

</div>